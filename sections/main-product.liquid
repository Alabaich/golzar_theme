  align-items: center;
  justify-content: center;
  border: 1px solid #d1d1d1;
  border-radius: 30px;
  padding: 0px 5px;
  background-color: white;
}

.custom-quantity-button {
  display: flex;
  align-items: center;
  justify-content: center;
  background: transparent;
  border: none;
  cursor: pointer;
  padding: 5px;
