<style>
.secondPartProduct{
  padding-top: 25px;
  display: flex;
  justify-content: space-between;
  align-items: start;
  gap: 25px;
}

  .product-tabs-container {
    padding:25px;
    width: 50%;
    border-radius: 25px;
    background: #fff;
  }
  .product-tab-links {
    display: flex;
    margin-bottom: 20px;
    gap: 20px;
  }
  .product-tab-link {
    display: flex;
    padding: 10px;
    cursor: pointer;
    border: none;
    font-size: 1em;
    border-radius: 15px;
    background-color: #F5F7F9;
    color: #808080;
    position: relative;
    transition: color 0.3s ease, background-color 0.3s ease;
  }
  .product-tab-link.active {
    background-color: #002642;
    font-weight: 600;
    color: #fff;
  }
  .product-tab-link:hover:not(.active) {
    color: #002642;
  }
  .product-tab-pane {
    display: none;
    padding: 15px 0;
    animation: fadeIn 0.5s ease-in-out;
  }
  .product-tab-pane.active {
    display: block;
  }
  .product-tab-pane h4 {
    margin: 0 0 15px 0;
    font-size: 1.1em;
    font-weight: 600;
  }
  .product-documents-list {
    display: flex;
    flex-direction: column;
    gap: 25px; /* Added gap between document sections */
  }
  .document-section ul {
      display: flex;
      flex-direction: column;
      gap: 15px;
      margin: 0;
      padding: 0;
      list-style: none;
  }
  .document-section a.customButton {
      margin: 0;
      display: inline-flex; /* Changed to inline-flex */
      align-items: center;
      gap: 10px; /* Adjusted gap */
      width: fit-content;
      text-decoration: none;
      padding: 8px 15px; 
      border-radius: 5px; 
  }
  .document-section a.customButton span {
      display: inline-block;
  }

  @keyframes fadeIn {
    from { opacity: 0; }
    to { opacity: 1; }
  }

  .secondPartProduct .rightSecondPart{
    width: 50%;
    display: flex;
    flex-direction: column;
    gap: 25px;
  }

  .secondPartProduct .rightSecondPart .shippingAndDelivery{
    padding: 25px;
    border-radius: 25px;
    background-color: #fff;
  }

  .secondPartProduct .rightSecondPart .shippingAndDelivery h4,
  .secondPartProduct .rightSecondPart .shippingAndDelivery p{
    margin: 0;
  }

  @media (max-width: 768px){
    .secondPartProduct{
      flex-direction: column;
      margin-top: 25px !important;
    }

    .product-tabs-container,
    .shippingAndDelivery{
    padding: 15px;
    }

      .product-tabs-container,
      .secondPartProduct .rightSecondPart{
    width: 100%;

  }

  .product__info-container{
    position: relative;
  }
  }
</style>
