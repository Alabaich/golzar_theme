            {{ product.description | truncate: 500, ". . ." }}
            <br><a class="prd_readmore" href="#">Read More</a>
          </div>
          <div class="product-description-full" style="display:none;">
            {{ product.description }}
            <br><a class="prd_readless" href="#">Read Less</a>
          </div>
          {% else %}
          {{ product.description }}
          {% endif %} 
        
          {% else %}
          {{ product.description }}
          {% endif %}
          </div>
        </div>
        {% endif %}
  
        {% if product.metafields.custom.spec_sheet != blank %}
