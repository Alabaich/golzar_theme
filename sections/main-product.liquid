<div class="page-width secondPartProduct">
  <div class="product-tabs-container">
    <div class="product-tab-links" role="tablist">
    {% assign is_first_active = true %}

    {% if show_description_tab %}
      <button class="product-tab-link {% if is_first_active %}active{% endif %}" data-target="product-description-content" role="tab" aria-selected="{% if is_first_active %}true{% else %}false{% endif %}">
        Description
      </button>
      {% assign is_first_active = false %}
    {% endif %}

    {% if show_documents_tab %}
      <button class="product-tab-link {% if is_first_active %}active{% endif %}" data-target="product-documents-content" role="tab" aria-selected="{% if is_first_active %}true{% else %}false{% endif %}">
        Documents
      </button>
      {% assign is_first_active = false %}
    {% endif %}

    {% if show_brand_tab %}
      <button class="product-tab-link {% if is_first_active %}active{% endif %}" data-target="product-brand-content" role="tab" aria-selected="{% if is_first_active %}true{% else %}false{% endif %}">
        Brand
      </button>
      {% assign is_first_active = false %}
    {% endif %}
  </div>

  <div class="product-tab-content">
    {% assign is_first_active = true %}

    {% if show_description_tab %}
      <div id="product-description-content" class="product-tab-pane {% if is_first_active %}active{% endif %}" role="tabpanel">
        <div class="product-description-content rte">
          {{ product.description 
  | replace: '&comma;', ',' 
  | replace: '&period;', '.' 
  | replace: '&lpar;', '(' 
  | replace: '&rpar;', ')' 
  | replace: '', '' 
  | replace: '&quest;', '?' 
  | replace: '&excl;', '!'
  | replace: '�', "'"
  | replace: '&colon;', ':'
}}
        </div>
      </div>
      {% assign is_first_active = false %}
    {% endif %}

    {% if show_documents_tab %}
      <div id="product-documents-content" class="product-tab-pane {% if is_first_active %}active{% endif %}" role="tabpanel">
        <div class="product-documents-list">

          {% if product.metafields.custom.spec_sheet != blank %}
            <div class="document-section spec-sheet-section">
              <h4>Product Spec Sheet</h4>
              <a href="{{ product.metafields.custom.spec_sheet | file_url }}" class="customButton blueButton" target="_blank" rel="noopener">
                <span>📥</span> Download PDF
              </a>
            </div>
          {% endif %}

          {% if product.metafields.custom.useful_links != blank %}
            <div class="document-section useful-links-section">
              <h4>Product Downloads & Manuals</h4>
              {% assign resource_links = product.metafields.custom.useful_links.value %}
              <ul>
                {% for link in resource_links %}
                  <li>
                    <a href="{{ link.url }}" class="customButton blueButton" target="_blank" rel="noopener">
                      <span>📥</span>
                      {% if link.text != blank %}
                        {{ link.text }}
                      {% else %}
                        Resource - {{ forloop.index }}
                      {% endif %}
                    </a>
                  </li>
                {% endfor %}
              </ul>
            </div>
          {% endif %}

          {% if product.metafields.custom.technical_drawing != blank %}
            <div class="document-section technical-drawing-section">
              <h4>Technical Drawing</h4>
              <a href="{{ product.metafields.custom.technical_drawing | file_url }}" class="customButton blueButton" target="_blank" rel="noopener">
                <span>📥</span> Download ZIP
              </a>
            </div>
          {% endif %}

        </div>
      </div>
      {% assign is_first_active = false %}
    {% endif %}

    {% if show_brand_tab %}
      <div id="product-brand-content" class="product-tab-pane {% if is_first_active %}active{% endif %}" role="tabpanel">
        <h4>About the Brand</h4>
        <div class="brand-description-content rte">
          {%- render 'brand-description', vendor_name: product.vendor -%}
          {% comment %} <p>Information about the brand {{ product.vendor }} will be displayed here.</p> {% endcomment %}
        </div>
      </div>
      {% assign is_first_active = false %}
    {% endif %}

  </div>
</div>
