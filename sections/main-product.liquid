<!-- The Hidden Quote Modal - Now a Liquid Contact Form -->
<div id="quote-modal" class="modal">
    <div class="modal-content">
        <span class="close-button" onclick="closeQuoteModal()">&times;</span>
        <h2>Ask for Quote</h2>
        <p>Please provide your contact and shipping information to receive a precise quotation for <b><span id="quote-product-title"></span></b>.</p>
        
        {% form 'contact', id: 'quote-form', class: 'modal-form' %}
            
            {% comment %} 
            This single hidden field aggregates all product and variant details 
            (Title, Handle, ID, SKU) to be included in the submission email body. 
            {% endcomment %}
            <input type="hidden" name="contact[Quote For Product]" id="quote-product-details-input">
            
            {% comment %} Required fields for the customer, named with the contact[] syntax {% endcomment %}
            <label for="first-name">First Name:</label>
            <input type="text" id="first-name" name="contact[First Name]" required>

            <label for="last-name">Last Name:</label>
            <input type="text" id="last-name" name="contact[Last Name]" required>

            <label for="email">Email:</label>
            <input type="email" id="email" name="contact[Email]" required>

            <label for="phone-number">Phone Number:</label>
            <input type="tel" id="phone-number" name="contact[Phone Number]">
            
            <label for="shipping-address">Shipping Address:</label>
            <input type="text" id="shipping-address" name="contact[Shipping Address]" required>

            <button type="submit">Submit Request</button>
            
            {% comment %} The success message is shown in JS after redirection {% endcomment %}
            <div id="quote-success-message" style="display: none; margin-top: 15px; color: #34C759; font-weight: bold;">
                Thank you! Your quote request has been sent. We will contact you shortly.
            </div>
        {% endform %}
    </div>
</div>


