<script>
document.addEventListener('DOMContentLoaded', function() {
  const quantitySelectors = document.querySelectorAll('.custom-quantity-selector');

  quantitySelectors.forEach(selector => {
    const input = selector.querySelector('.custom-quantity-input');
    const btnMinus = selector.querySelector('.custom-quantity-button[name="minus"]');
    const btnPlus = selector.querySelector('.custom-quantity-button[name="plus"]');
    const min = parseInt(input.min, 10) || 1;

    btnPlus.addEventListener('click', function() {
      let currentValue = parseInt(input.value, 10);
      input.value = currentValue + 1;
    });

    btnMinus.addEventListener('click', function() {
      let currentValue = parseInt(input.value, 10);
      if (currentValue > min) {
        input.value = currentValue - 1;
      }
    });

    input.addEventListener('keydown', function(e) {
      if (!((e.keyCode > 95 && e.keyCode < 106)
        || (e.keyCode > 47 && e.keyCode < 58)
        || e.keyCode === 8)) {
          e.preventDefault();
      }
    });
  });

  document.querySelectorAll('form[method="post"][action*="/cart/add"]').forEach(form => {
    form.addEventListener('submit', function(e) {
      e.preventDefault();
      
      const variantId = this.querySelector('.variantss-id').value;
      const quantityInput = this.querySelector('.custom-quantity-input');
      const quantity = quantityInput ? parseInt(quantityInput.value) : 1;
      
      const formData = new FormData();
      formData.append('id', variantId);
      formData.append('quantity', quantity);
      
      fetch('/cart/add.js', {
        method: 'POST',
        body: formData,
        headers: {
          'Accept': 'application/json'
        }
      })
      .then(response => response.json())
      .then(data => {
        console.log('Added to cart:', data);
      })
      .catch(error => {
        console.error('Error:', error);
      });
    });
  });
});
</script>

