                    <script>
                      function copySkuToClipboard(skuElement) {
                  if (!skuElement) {
                      console.error('SKU element not found.');
                      return;
                  }

                  const sku = skuElement.textContent.trim(); // Get the text content and remove leading/trailing whitespace
                  const originalText = skuElement.textContent; // Store original text for restoration

                  // Use the modern Clipboard API
                  if (navigator.clipboard && navigator.clipboard.writeText) {
                      navigator.clipboard.writeText(sku)
                          .then(() => {
                              // Provide visual feedback to the user
                              skuElement.textContent = 'Copied!';
                              setTimeout(() => {
                                  skuElement.textContent = originalText; // Revert text after a short delay
                              }, 1500); // Display "Copied!" for 1.5 seconds
                              console.log('SKU copied to clipboard:', sku);
                          })
                          .catch(err => {
                              console.error('Failed to copy SKU:', err);
                              // Fallback for older browsers or if permission is denied
                              fallbackCopyTextToClipboard(skuElement, sku, originalText);
                          });
                  } else {
                      // Fallback for older browsers (less reliable and might show a prompt)
                      console.warn('Clipboard API not supported. Using fallback method.');
                      fallbackCopyTextToClipboard(skuElement, sku, originalText);
                  }
              }

// Fallback function for older browsers that don't support navigator.clipboard.writeText
function fallbackCopyTextToClipboard(skuElement, text, originalText) {
    const textArea = document.createElement('textarea');
    textArea.value = text;

    // Make the textarea out of sight
    textArea.style.position = 'fixed';
    textArea.style.top = '0';
    textArea.style.left = '0';
    textArea.style.width = '2em';
    textArea.style.height = '2em';
    textArea.style.padding = '0';
    textArea.style.border = 'none';
    textArea.style.outline = 'none';
    textArea.style.boxShadow = 'none';
    textArea.style.background = 'transparent';

    document.body.appendChild(textArea);
    textArea.focus();
    textArea.select();

    try {
        const successful = document.execCommand('copy');
        const msg = successful ? 'Copied!' : 'Failed to copy!';
        skuElement.textContent = msg;
        setTimeout(() => {
            skuElement.textContent = originalText;
        }, 1500);
        console.log('Fallback copy SKU ' + msg + ':', text);
    } catch (err) {
        console.error('Fallback: Oops, unable to copy', err);
        skuElement.textContent = 'Error!';
        setTimeout(() => {
            skuElement.textContent = originalText;
        }, 1500);
    }

    document.body.removeChild(textArea);
}
      </script>
