                  <p {% if product.metafields.custom.delivery_time == 'Call For Availability' or product.metafields.custom.delivery_time == 'Chat with us for availability' %} class="trigger_whatsapp_chat"{% endif %}>{{ product.metafields.custom.delivery_time }}</p>
                </div>
              </div>
              {% endif %}

              {%- when 'variant_picker' -%}
                {% render 'product-variant-picker', product: product, block: block, product_form_id: product_form_id %}
              {%- when 'buy_buttons' -%}
