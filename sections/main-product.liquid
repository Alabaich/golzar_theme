          <h2>Product Spec Sheet</h2>
          <p>📥 <a href="{{ product.metafields.custom.spec_sheet | file_url }}" target="_blank" rel="noopener">Download PDF</a></p>
        </div>
        {% endif %}
  
        {% if product.metafields.custom.technical_drawing != blank %}
        <div class="tab-pane file_download" id="tab3">
          <h2>Technical Drawing:</h2>
          <p>📥 <a href="{{ product.metafields.custom.technical_drawing | file_url }}" target="_blank" rel="noopener">Download ZIP</a></p>
        </div>
        {% endif %}
  
      </div>
  </div>

</div>
{% endif %}

